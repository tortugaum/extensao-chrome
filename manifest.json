{
  "name": "Inno Ext",
  "description": "Extens√£o Innovation",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/"],
      "css": ["styles.css"],
      "js": ["script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.html", "assets/*"],
      "matches": ["https://web.whatsapp.com/*"]
    }
  ],
  "icons": {
    "16": "assets/16.png",
    "32": "assets/32.png",
    "48": "assets/48.png",
    "64": "assets/64.png",
    "128": "assets/128.png"
  }
}
